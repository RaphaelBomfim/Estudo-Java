package Classe;

public class DataTeste {

	public static void main(String[] args) {
		
		Data d1 = new Data();
	    Data d2 = new Data();
	    
	    d1.dia = 21;
	    d1.mes = 03;
	    d1.ano = 1996;
	    
	    d2.dia = 07;
	    d2.mes = 10;
	    d2.ano = 1972;
	    
	    
	    System.out.printf("%d/%d/%d", d1.dia, d1.mes, d1.ano);
		
	}
	
	
	
	
}
